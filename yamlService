
import Foundation
import OSLog

class YAMLService {
    var yamlCatalog: YAMLcatalog?
    private let logger = Logger(subsystem: "YAMLParsing", category: "Model")

    init() {
        self.yamlCatalog = yamlParses()
    }
}

extension YAMLService {
    func yamlParses() -> YAMLcatalog? {
        guard let url = Bundle.main.url(forResource: "fileName", withExtension: "xml") else { return nil }
        do {
            let data = try Data(contentsOf: url)
            let parser = YAMLParser(data: data)
            if parser.parse() {
                return parser.yamlCatalog
            } else {
                print("NSXML Parsing error")
                return nil
            }
        } catch {
            print("Data read error:\(error.localizedDescription)")
            return nil
        }
    }

}
